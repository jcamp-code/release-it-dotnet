{
  "name": "release-it-dotnet",
  "type": "module",
  "version": "0.1.1",
  "description": "NuGet publish and DotNet version read/write plugin for release-it",
  "author": "John Campion Jr (https://github.com/JohnCampionJr/)",
  "license": "MIT",
  "repository": "https://github.com/jcamp-code/release-it-dotnet",
  "keywords": [
    "release",
    "release-it",
    "release-it-plugin",
    "version",
    "bump",
    "dotnet"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": {
    "release-it-dotnet": "./dist/bin/release-it-dotnet.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "test": "vitest run",
    "release": "unbuild & release-it",
    "pre": "release-it minor --preRelease=beta",
    "cl": "changelogen"
  },
  "peerDependencies": {
    "changelogen": "^0.5.7",
    "release-it": "^17.10.0",
    "semver": "^7.6.3"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.4",
    "chalk": "^5.3.0",
    "changelogen": "^0.5.7",
    "defu": "^6.1.4",
    "detect-indent": "^7.0.1",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "globby": "^14.0.2",
    "js-yaml": "^4.1.0",
    "jsonpath": "1.1.1",
    "pathe": "^1.1.2",
    "release-it": "^17.10.0",
    "release-it-changelogen": "^0.1.0",
    "rimraf": "^5.0.10",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "xpath": "^0.0.34",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@jcamp/eslint-config": "^1.0.4",
    "@types/node": "^20.17.0",
    "@types/semver": "^7.5.8",
    "eslint": "8.57.1",
    "knip": "^2.43.0",
    "prettier": "3.3.3",
    "sinon": "^17.0.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  }
}
